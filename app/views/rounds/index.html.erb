<%= link_to current_user.email, edit_user_registration_path, :class => '#' %> |
<%= link_to "Logout", destroy_user_session_path, method: :delete, :class => '#'  %>
<h1>Rounds</h1>

Handicap: <%= @handicamp %>

<table>
  <thead>
    <tr>
      <th>Course</th>
      <th>Rating</th>
      <th>Slope</th>
      <th>Score</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rounds.each do |round| %>
      <tr>
        <td><%= round.course %></td>
        <td><%= round.rating %></td>
        <td><%= round.slope %></td>
        <td><%= round.score %></td>
        <td><%= link_to 'Edit', edit_round_path(round) %></td>
        <td><%= link_to 'Destroy', round, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Round', new_round_path %>
